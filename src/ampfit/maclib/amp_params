"macro amp_params"

mkampargs

$tn=tn
$rfchnl=ampargs[2]
$mode=ampargs[10]
$itr=ampargs[7]
$tpwr1=tpwr1
$tpwr3=tpwr3
if $#>0 then
   if $1='fidelity' then
      rtpar('fidelity')
   endif
   if $1='tpwrmap' then
      rtpar('tpwrmap')
   endif
endif

tn=$tn
ampargs[2]=$rfchnl
ampargs[10]=$mode
ampargs[7]=$itr
tpwr1=$tpwr1
tpwr3=$tpwr3



"macro ATadiab"
ATwft
if (celem>(arraydim/2)) then
 vp=0 select(20) vsadj(30) dssh(1,41) 
 write('graphics',0,20,'WURST  Modulation')
 vp=vp+60 dsshn(42,celem)
 write('graphics',0,vp+20,'STUD Modulation')
 dtext
else
if (celem>arraydim/4) then
 select(20) vsadj(30) dssh 
 write('graphics',0,vp+20,'WURST Modulation')
else
 select(1) vsadj(10) dssh dtext
 write('graphics',0,20,'WURST Modulation')
endif endif

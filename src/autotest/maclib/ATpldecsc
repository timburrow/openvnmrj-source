"macro ATpldecsc"
"ATpldecsc"
 ph  "put into phased mode"
if (at_plotauto='y') then
 if (arraydim=41) then
 if (at_printparams='y') then
   dn2='' pap ATpltext 
   pps(120,0,wcmax-120,90)
   ATpage 
 endif
    select(21) aph0 vsadj(100)
    vp=0 dssh(1,41) pl(1,41)
    write('plotter',20,vp+20,'Waltz16 Modulation')
    write('graphics',20,vp+20,'Waltz16 Modulation')
    ATpltext(wcmax-80,wc2max)
 endif
 ATpage 
endif

"macro rna_auto2d4a"
atext('  \\Imino Nitrogens Optimized')
BPsvf(userdir+'/AutoRNA_H2O_2D/rna_CPMGgNhsqc_imino')
$d1=d1 $nt=nt
rna_gNhmqc
imino='y' amino='n' 
sw=22*sfrq np=2048 sw1=25*dfrq2 diff=8*sfrq
ss=8 d1=$d1 nt=$nt ni=16 phase=1,2
setref
crl1 rfl1=sw1/2 rfp1=155*dfrq2 f
rna_2Dtime
wexp='rna_auto2d5' au

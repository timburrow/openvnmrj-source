"macro rna_autojumpret"
"rna_autojumpret"
BPgettemp:$temp
BPrtppar('water') jumpret='y'
presat='n' shaped='n' wet='n' watergate='n' temp=$temp
gt0=.002 jrp1=pw90 jrpw=pw90
sspul='y' d1=1.5 nt=2 gain=12
temp=$temp
array('jrpw',11,pw90-.1,0.025)
ssfilter='n' 
wexp='rna_autojumpretproc1' wnt='f wft select(1) av vsadj dssh'
dg
av
atext('automatic jumpreturn ') dps dtext au

"macro BPauto1d18"
"BPauto1d18 (from BP23)"
$nt=nt
setref wft ph
 f full
BPaph
cz intmod='off' f full vsadj(80) ds dscale
write('plotter',50,wc2max-5,'Optimized HCACON First Increment')
wc=0.7*wc vp=0
pps(120,90,wcmax-120,wc2max-90)
pl pscale pap BPpage
BPprotref1(174,'dfrq')
BPprotref2(56,'dfrq')
BPsvf(userdir+'/AutoTripRes1D/ghca_co_n_1D')

$temp=temp
ghca_co_canh
ss=4 nt=$nt  temp=$temp

wnt='' wexp='BPauto1d19' au
dps

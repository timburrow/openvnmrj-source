"macro BPlist"

" BPlist(option) - show or print out BioPacklist "

clear
if ($1)=2 then 
   printon cat('/vnmr/biopack/manual/BioPacklist') printoff
   cat('/vnmr/biopack/manual/BioPacklist')
   write('line3','List is printed. Select TextOut Page to View List')
endif
if ($1)=1  then
   cat('/vnmr/biopack/manual/BioPacklist')
   write('line3','Select TextOut Page to View List')
 endif

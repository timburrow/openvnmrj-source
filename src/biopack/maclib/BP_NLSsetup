"macro BP_NLSsetup"
exists('sampsched','parameter'):$e
if $e=0 then
create('sampsched','string')
endif
exists('t1_counter','parameter'):$e
if $e=0 then
create ('t1_counter','integer')
endif
exists('t2_counter','parameter'):$e
if $e=0 then
create ('t2_counter','integer')
endif
exists('nli','parameter'):$e
if $e=0 then
create ('nli','integer')
endif
exists('nli2','parameter'):$e
if $e=0 then
 create ('nli2','integer')
endif

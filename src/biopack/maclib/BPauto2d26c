"macro BPauto2d26c"
" BPauto2d26c "
BPsvf(userdir+'/AutoTripRes2D/ghca_co_canh_NH_2D')
$d1=d1 $nt=nt
ghnha
d1=$d1
ni=128 phase=1,2 nt=$nt/4
calcdim text:n2
$time=arraydim*nt*(d1+at)/60 "approx min"
format($time,2,1):n3
n3=n3+' minutes'
atext('automatic HH 2D experiment taking')
atext(n3)

wexp='BPauto2d27' au

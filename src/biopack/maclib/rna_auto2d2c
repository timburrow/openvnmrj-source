"macro rna_auto2d2c"
text('  \\Aromatic Carbons')
BPsvf(userdir+'/AutoRNA_H2O_2D/rna_gChsqc_CTSE_aromatics')
$nt=nt $d1=d1
rna_gNhsqc
d1=$d1
imino='y' amino='n' NH2only='n'
crl1 setref
sw=22*sfrq np=2048 sw1=25*dfrq2
ss=8 nt=$nt ni=16 phase=1,2
rfl1=sw1/2 rfp1=155*dfrq2  
rna_2Dtime
wexp='rna_auto2d3' au

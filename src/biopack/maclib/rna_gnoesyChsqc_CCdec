"macro rna_gnoesyChsqc_CCdec"
"rna_gnoesyChsqc_CCdec macro for BioPack, Jan 2008"

BPrtppar('rna_gnoesyChsqc_CCdec')

dm='nnny' dm2='nnnn'
getparam('NHgstab','N15'):gstab

dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
BPpboxget('wurst80.DEC'):dseq,$dmmy,dpwr,$dmmy,dres,dmf

BPrna_CCdec
ddrtc=alfa
gt5=1/(4*JCH)-0.001 -gstab   "shorten gt5 enough for gstab"
ni=0 phase=1
wnt='' wexp='' spin='n'
man('rna_gnoesyChsqc_CCdec')
 

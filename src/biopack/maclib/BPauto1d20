"macro BPauto1d20"
"BPauto1d20 "
$nt=nt
wft ph
setref f full
BPaph
cz intmod='off' f full vsadj(80) ds dscale
write('plotter',50,wc2max-5,'Optimized HNHA First Increment')
wc=0.7*wc vp=0
pps(120,90,wcmax-120,wc2max-90)
pl pscale pap BPpage
BPsvf(userdir+'/AutoTripRes1D/ghnha_1D')

$temp=temp
ghnhb
ss=4 nt=$nt  temp=$temp

wnt='' wexp='BPauto1d21' au
dps

"macro BP_3Dnoft"
"BP_3Dnoft - show a 3D using ft3d('noft') facility"

if($#>0) then 
  rt($1)
endif

ft('noft',1)  " make sure the parameters get loaded "
proc1='' proc2='' proc=''
f1coef='' f2coef=''
fn=np fn1='n' fn2='n'
ssfilter='n'
sb='n' sb1='n' sb2='n'
gf='n' gf1='n' gf2='n'
lb='n' lb1='n' lb2='n'

ft3d('noft','f1f3','f2f3')
vs2d=1.0 trace='f3'
banner('wait for ft3d to finish data formatting')



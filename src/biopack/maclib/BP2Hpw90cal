"macro BP2Hpw90cal"
"BPpw90cal"

av
$counter=1
select(1) vsadj(90)
$bestheight=90
$bestindex=1
repeat
   select($counter)
   peak:$ht
   if ($ht<$bestheight) then
      $bestheight=$ht
      $bestindex=$counter
   endif
   $counter=$counter+1
until $counter>arraydim
return({$1}[$bestindex]/4.0,$bestindex)

"macro rna_gNhmqc"
"rna_gNhmqc macro for BioPack, July 2002"

BPrtppar('rna_gNhmqc')
BPsetampmode sw=20p fn=2*np
 getparam('NHgstab','N15'):grecov
 getparam('swN','N15'):sw2 
 dm2='nny' imino='y' amino='n'
 C13refoc='y' dpwr=0
 BPsetwurstparams
if at>0.21 then at=.2 endif
dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
wnt='' wexp='' spin='n'
ni=0 phase=1
man('rna_gNhmqc')

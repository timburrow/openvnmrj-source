"macro rna_HCN"
"rna_HCN macro for BioPack, July 2002"

BPrtppar('rna_HCN')
wet='y'
BPsetampmode sw=10p fn=2*np
getparam('N15gstab','N15'):grecov
dm='nny' dm2='nnn' axis='p2d'
getparam('wetpwr','H1'):wetpwr
getparam('wetpw','H1'):wetpw
getparam('wetshape','H1'):wetshape
sw1=40*dfrq2 sw2=20*dfrq
dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
crl1 rfl1=sw1/2 rfp1=155*dfrq2
crl2 rfl2=sw2/2 rfp2=115*dfrq
BPsetwurstparams

ni=0 phase=1
wnt='' wexp='' spin='n'
man('rna_HCN')

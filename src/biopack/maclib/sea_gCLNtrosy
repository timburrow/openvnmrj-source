 getparam('swN','N15'):sw1 
 dm2='nnn' dm='nnn' 
 tofPM=tof+(8-4.67)*sfrq  "move tof 8ppm for CLEANEX"

 d3=.00022*(500.0/sfrq)  "typical value"
 
 getparam('gt0','N15'):gt0
 getparam('gzlvl0','N15'):gzlvl0

gt5=1/(4.0*JNH)-pwHs*1.0e-6 -gstab -0.0001
wnt='' wexp='' spin='n'
BPprotref1(120,'dfrq2')
ni=1 phase=1
if Console='vnmrs' then ddrtc=alfa endif
 

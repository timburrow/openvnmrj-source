"macro rna_auto2d5a"
atext('  \\Amino Nitrogens Optimized')
BPsvf(userdir+'/AutoRNA_H2O_2D/rna_gNhmqc_amino')
$d1=d1 $nt=nt
rna_WGgNhsqc
sw=22p np=2048
imino='n' amino='n' Naro='y' JNH=22
sw1=90*dfrq2
ss=8 d1=$d1 nt=$nt ni=32 phase=1,2
setref crl1 rfl1=sw1/2 rfp1=200*dfrq2 f
rna_2Dtime
wexp='rna_auto2d6a' au

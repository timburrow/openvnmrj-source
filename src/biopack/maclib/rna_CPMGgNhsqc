"macro rna_CPMGgNhsqc"
"rna_CPMGgNhsqc macro for BioPack, July 2002"

BPrtppar('rna_CPMGgNhsqc')
BPsetampmode
 sw=20p fn=2*np 
 getparam('NHgrad1_time','N15'):gt1 
 getparam('NHgrad1_lvl','N15'):gzlvl1 
 getparam('NHgrad2_lvl','N15'):gzlvl2 
 getparam('NHgstab','N15'):grecov
 getparam('swN','N15'):sw1 
 dm2='nny' dm='nnn'
 amino='y' imino='n' ncyc=16
 BPsetwurstparams

 dof2=dof2+80*dfrq2
 dof=dof-(174-110)*dfrq

wnt='' wexp='' spin='n'

ni=0 phase=1

man('rna_CPMGgNhsqc')

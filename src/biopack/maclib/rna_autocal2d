"macro rna_autocal2d"
f $bestone=1 $counter=1
select(1) vsadj(50) $best=50
repeat
  select($counter)
  peak:$ht
  if $ht>$best then $best=$ht $bestone=$counter endif
  $counter=$counter+1
until $counter>arraydim
BPsvf(userdir+'/AutoRna/pwC90cal_highpower_coarse')
write('plotter',100,wc2max,'Determination of 13C pw90 at pwClvl (coarse)')
select($bestone) vsadj(50)  dssh('dodc') pl('all','dodc') pap BPpage 
pwC=pwC[$bestone] 
array('pwC',21,1.3*pwC,-0.03*pwC)
wexp='rna_autocal2e' au

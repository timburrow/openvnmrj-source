"macro satxfer1D"

BPrtppar('satxfer1D')
getparam('NHgstab','N15'):gstab
getparam('tofH2O','H1'):tof
getparam('tofH2O','H1'):satfrq
getparam('tofH2O','H1'):selfrqs
selfrq1=tof+25p
selfrq=tof-6.6p
getparam('tpwr','H1'):tpwr
getparam('pw90','H1'):pw
slpwr=tpwr-20 slpw=10*pw*compH
selpwr=tpwr-66 selpw=2000*pw*compH*2.4 "factor of 2.4 hard->gauss"
selpwrs=tpwr-42 selpws=250*pw*compH*2.4 "makes gaus180"
selshapes='gauss'
selshape='gauss'
satmode='yy' satpwr=tpwr-56
wexp='' wnt='' spin='n'


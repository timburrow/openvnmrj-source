"macro rna_HCP"
"rna_HCP macro for BioPack, Nov 2002"

BPrtppar('rna_HCP')
wet='n' WATER='n'
BPsetampmode sw=10p fn=2*np
getparam('N15gstab','N15'):grecov
dm='nny' dm2='nnn' axis='pd2'
getparam('wetpwr','H1'):wetpwr
getparam('wetpw','H1'):wetpw
getparam('wetshape','H1'):wetshape
sw1=8*dfrq sw2=40*dfrq2
dof=dof-(174-110)*dfrq
crl2 rfl2=sw2/2 rfp2=110*dfrq
BPsetwurstparams

ni=0 phase=1
wnt='' wexp='' spin='n'
man('rna_HCP')
banner('Set P31 Pulse and Decoupling Parameters Manually')

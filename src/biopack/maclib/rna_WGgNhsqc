setfrq BPsetampmode reffrq=sfrq-(sw/2-rfl+rfp)/1e6
sw=20p fn=2*np sw1=85*dfrq
 getparam('NHgstab','N15'):grecov
 dm2='nny' C13refoc='y' dpwr=0
dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
JNH=15
wnt='' wexp='' spin='n'

ni=0 phase=1

man('rna_WGgNhsqc')

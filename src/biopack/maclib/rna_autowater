"macro rna_autowater"
if (presat='y') then rna_autopresat endif
if (shaped='y') then rna_autoshaped endif
if ((watergate='y') and (flagsoft='y')) then 
 rna_autowatergate(0) endif
if ((watergate='y') and (flag3919='y') ) then
 rna_autowatergate(1) endif
if ((watergate='y') and (flagW5='y')) then
 rna_autowatergate(2) endif
if (jumpret='y') then rna_autojumpret endif
if (wet='y') then rna_autowet endif

"macro BPauto1d16a"
"BPauto1d16a "
$nt=nt
setref wft ph lp=0 BPaph
f full
f full vsadj(80) ds dscale
write('plotter',50,wc2max-5,'Optimized HCCHCOSY First Increment')
wc=0.7*wc vp=0

pps(120,90,wcmax-120,wc2max-90)
pl pscale pap BPpage
crl1 rfl1=sw1/2 rfp1=rfp reffrq1=reffrq
BPprotref2(46,'dfrq')
BPsvf(userdir+'/AutoTripRes1D/hcch_cosy_1D')

$temp=temp

ghca_co
temp=$temp nt=$nt ss=4

wexp='BPauto1d17'  au
dps

"macro gNhmqcJ"
"gNhmqcJ macro for BioPack, June 2004"

BPrtppar('gNhmqcJ')

 getparam('swN','N15'):sw1 
 getparam('JNH','N15'):jxh 
 dm2='nny' 
 dpwr=0

 
getparam('tpwr','H1'):tsatpwr 
tsatpwr=tsatpwr-60
tpwrmess=tpwr-20

getparam('pwHs','H1'):pw_sl 
ln(pw_sl/(compH*pw)):$ln
tpwrsl=tpwr - 20.0*$ln/2.303
tpwrsl=trunc(tpwrsl+0.5)
shp_sl='hard'

wnt='' wexp='' spin='n'
ni=0 phase=1
man('gNhmqcJ')

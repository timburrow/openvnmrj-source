"macro BPauto2d12BEST"
" BPauto2d12BEST "
$d1=d1 $nt=nt $at=at
BPsvf(userdir+'/Auto2DBEST/hncacb_NH_2D')
best_hncocacbP
  ni=32 d1=$d1 phase=1,2 nt=$nt at=$at
  calcdim text:n2

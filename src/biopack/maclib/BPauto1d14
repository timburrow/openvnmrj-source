"macro BPauto1d14"
"BPauto1d14 (from BP17)"
$nt=nt
wft ph
setref f full
BPaph0 f full vsadj(80) ds dscale
write('plotter',50,wc2max-5,'Optimized CBCA(CO)NH First Increment')
wc=0.7*wc vp=0
pps(120,90,wcmax-120,wc2max-90)
pl pscale pap BPpage
BPprotref1(46,'dfrq')
BPprotref2(120,'dfrq2')
BPsvf(userdir+'/AutoTripRes1D/gcbca_co_nh_1D')
$temp=temp
ghcch_tocsy
ss=4  temp=$temp
nt=$nt
"wnt='' wexp='BP18'"
wnt='' wexp='BPauto1d15'
au
dps

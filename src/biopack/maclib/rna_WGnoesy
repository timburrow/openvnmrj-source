"macro rna_WGnoesy"

BPrtppar('rna_WGnoesy')
sw=20p sw1=sw p180=2.0*pw fn=2*np
getparam('NHgstab','N15'):gstab
gstab0=gstab gstab1=gstab gstab2=gstab
dof2=dof2+35*dfrq2
dof=dof-(174-110)*dfrq
dm='nnnn' dm2='nnnn'
BPcheckdpwr2
if dmm<>'ccp' then
  dpwr=pwClvl-15 dmf=1e6/(2*2*1.4*pwC*compC)
  BPcheckdpwr
else
  BPsetwurstparams
endif
wexp='' wnt='' spin='n'
ni=1 phase=1
man('rna_WGnoesy')


"macro BPautowgtocsy"
"BPautowgtocsy"
"assumes watergate optimization is done and results"
"are in current experiment"
seqfil='wgtocsy'
par_2d
create('gzlvl3','real') gzlvl3=43
create('strength','real') strength=5000
create('mix','delay') mix=.06
create('gzlvl0','real') gzlvl0=6000
create('gt0','delay') gt0=.001
create('gzlvl2','real') gzlvl2=600
create('gt2','delay') gt2=.001
create('pwHs','pulse')

"macro WGgNhsqc"
"WGgNhsqc macro for BioPack, July 2002"

BPrtppar('WGgNhsqc')
setfrq reffrq=sfrq-(sw/2-rfl+rfp)/1e6

 getparam('NHgstab','N15'):grecov
 getparam('swN','N15'):sw2 
 dof=dof-(174-56)*0.5*dfrq
 dm2='nny' C13refoc='y' dpwr=0
 H1refoc='y'

 
 getparam('gt5','N15'):gt5
 getparam('gzlvl5','N15'):gzlvl5

wnt='' wexp='' spin='n'

ni=0 phase=1

man('WGgNhsqc')

"macro CTgChmqc"
"CTgChmqc macro for BioPack, July 2002"

BPrtppar('CTgChmqc')

 getparam('NHgstab','N15'):grecov
 dof=dof-(174-35)*dfrq sw1=80*dfrq
 if dmm<>'ccp' then
   dpwr=pwClvl-15 dmf=1e6/(2*2*1.4*pwC*compC)
   BPcheckdpwr
 else
   BPsetwurstparams
 endif

ni=0 phase=1
BPsetwurstparams

wnt='' wexp='' spin='n'
SHAPE='y' aliph='y'
ni=0 phase=1

man('CTgChmqc')
 

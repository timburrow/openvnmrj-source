"macro BPcallpage"
"BPcallpage   forces page display"  
if ($#=0) then
  vnmrjcmd('setpage','Display')
  vnmrjcmd('setpage','Display','1DOptions.xml')
elseif ($1=1) then
  vnmrjcmd('setpage','Acquire')
  vnmrjcmd('setpage','Acquire','BioPackOptions.xml')
elseif ($1=2) then
  vnmrjcmd('setpage','Acquire')
  vnmrjcmd('setpage','Acquire','BioPackCalibrations.xml')
endif
  

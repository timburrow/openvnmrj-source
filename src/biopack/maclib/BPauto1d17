"macro BPauto1d17"
"BPauto1d17 (from BP22)"
$nt=nt 
setref wft
 f full
BPaph
f full vsadj(80) ds dscale
write('plotter',50,wc2max-5,'Optimized HCACO First Increment')
wc=0.7*wc vp=0
pps(120,90,wcmax-120,wc2max-90)
pl pscale pap BPpage
BPprotref1(174,'dfrq')
BPprotref2(56,'dfrq')
BPsvf(userdir+'/AutoTripRes1D/ghca_co_1D')
$temp=temp
ghca_co_n
ss=4 nt=$nt  temp=$temp

wnt='' wexp='BPauto1d18' au
dps

"macro rna_gChmqc"
"rna_gChmqc macro for BioPack, July 2002"

BPrtppar('rna_gChmqc')
dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
BPsetampmode
N15refoc='y' aromatic='y' ribose='n' wet='y'
dm='nny' dm2='nnn'
getparam('NHgstab','N15'):grecov
getparam('wetpwr','H1'):wetpwr
getparam('wetpw','H1'):wetpw
BPsetwurstparams

dof=dof-(174-110)*dfrq
dof2=dof2+80*dfrq2
wnt='' wexp='' spin='n'
ni=0 phase=1
man('rna_gChmqc')




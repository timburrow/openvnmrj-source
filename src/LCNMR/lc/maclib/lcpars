setgroup('lcEluteIndex','processing','global')
setprotect('lcEluteIndex','set',32768+16+65536,'global')

exists('lcCr1','parameter','global'):$e
if (not $e) then
    create('lcCr1','real','global')
    lcCr1=0
endif
setgroup('lcCr1','processing','global')
setprotect('lcCr1','set',32768+16+65536,'global')

exists('lcCr2','parameter','global'):$e
if (not $e) then
    create('lcCr2','real','global')
    lcCr2=0
endif
setgroup('lcCr2','processing','global')
setprotect('lcCr2','set',32768+16+65536,'global')

exists('lcColumnValve','parameter','global'):$e
if (not $e) then
    create('lcColumnValve','real','global')
    lcColumnValve=0
endif
setgroup('lcColumnValve','processing','global')
setprotect('lcColumnValve','set',32768+16+65536,'global')


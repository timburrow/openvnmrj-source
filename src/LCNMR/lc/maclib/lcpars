//
// A space-separated list of parameters making up an MS method
//
exists('msMethodParameters','parameter','global'):$e
if (not $e) then
    create('msMethodParameters','string','global')
endif
setgroup('msMethodParameters','processing','global')
setprotect('msMethodParameters','set',32768+16+65536,'global')
$pars='msCidGasOn msCapillaryVoltage msDetectorVoltage'
$pars=$pars+' msScanTime msPolarity'
$pars=$pars+' msQ1MassStart msQ1MassEnd msQ3MassStart msQ3MassEnd'
$pars=$pars+' msCollisionEnergy msQ1PeakWidth msQ3PeakWidth'
msMethodParameters=$pars


//
// A space-separated list of parameters making up an MS mode
//

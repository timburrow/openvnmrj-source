// Holds the peak detect enable info for all traces for the current time
exists('lcPeakDetect','parameter','global'):$e
if (not $e) then
    create('lcPeakDetect','string','global')
    setgroup('lcPeakDetect','processing','global')
    lcPeakDetect='n','n','n','n','n'
endif
setenumeral('lcPeakDetect',2,'y','n','global')
setgroup('lcPeakDetect','processing','global')
setprotect('lcPeakDetect','set',32768+16+65536,'global')
$size = size('lcPeakDetect')
while ($size < lcMaxNumChans) do
    $size = $size + 1
    lcPeakDetect[$size] = 'n'
endwhile


// Holds the peak detect enable info for a single trace for the whole method
exists('lcPeakDetectA','parameter','global'):$e
if (not $e) then
    create('lcPeakDetectA','string','global')
    lcPeakDetectA='n'

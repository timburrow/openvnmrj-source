setgroup('lcBandwidth', 'processing','global')
setprotect('lcBandwidth','set',32768+16+65536,'global')
setlimit('lcBandwidth', 4, 0, 1, 'global')


exists('lcThreshold','parameter','global'):$e
if ($e) then
    if (typeof('lcThreshold')) then
        destroy('lcThreshold','global')
        $e=0
    endif
endif
if (not $e) then
    create('lcThreshold','real','global')
    setgroup('lcThreshold','processing','global')
    lcThreshold=0,0,0,0,0
endif
setgroup('lcThreshold','processing','global')
setprotect('lcThreshold','set',32768+16+65536,'global')
$size = size('lcThreshold')
while ($size < lcMaxNumChans) do
    $size = $size + 1
    lcThreshold[$size] = 0
endwhile


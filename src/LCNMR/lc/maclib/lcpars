exists('lcHoldType','parameter','global'):$e
if (not $e) then
    create('lcHoldType','string','global')
    lcHoldType='fixed'
endif
setgroup('lcHoldType','processing','global')
setprotect('lcHoldType','set',32768+16+65536,'global')
setenumeral('lcHoldType', 3, 'nmr', 'indefinite', 'fixed', 'global')




//
// A space-separated list of parameters making up an LC method
//
exists('lcMethodParameters','parameter','global'):$e
if (not $e) then
    create('lcMethodParameters','string','global')
endif
setgroup('lcMethodParameters','processing','global')
setprotect('lcMethodParameters','set',32768+16+65536,'global')
$pars='lcMethodRows lcTime lcPercentC lcPercentB lcPercentA'
$pars=$pars+' lcFlowRate lcAdcPeriod lcPdaPeriod_ms'
$pars=$pars+' lcTrace lcTraceLambda'    // NB: Arrayed
$pars=$pars+' lcLambda lcLambda2'       // UV monitor lambdas
$pars=$pars+' lcLambdaMin lcLambdaMax lcBandwidth'
$pars=$pars+' lcPeakDetectA lcPeakDetectB lcPeakDetectC lcPeakDetectD lcPeakDetectE'
$pars=$pars+' lcThresholdA lcThresholdB lcThresholdC lcThresholdD lcThresholdE'
$pars=$pars+' lcTimeSlice lcTimeSlicePeriod lcRelay1 lcRelay2 lcNmrBypass'
$pars=$pars+' lcRelay3 lcRelay4 lcRelay5 lcRelay6'
$pars=$pars+' lcCommand lcPostRunCommand lcPeakCommand lcHoldTime lcHoldType'
$pars=$pars+' lcColumnType lcColumnId lcColumnLength'
$pars=$pars+' lcColumnWidth lcColumnDeadVolume lcColumnParticleSize'
$pars=$pars+' lcMinPressure lcMaxPressure lcEquilTime'
$pars=$pars+' lcMethodComment solvA solvB solvC'
$pars=$pars+' msPump msValve msScans'
lcMethodParameters=$pars + ' lcexpGlobal'


//
// Method Variables

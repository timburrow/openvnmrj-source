"macro lcpars"
/*-*- MAGICAL -*-*/
"macro lcpars"
$version = '21.1'        // SCCS version number

// Note: These LC parameters are global because they are experiment
// independent; the user can join an experiment or retrieve parameters
// while keeping the LC method the same.
// However, the parameters are not shared between VnmrBGs; different
// workspaces can have different LC methods showing.
// However, only one workspace at a time can communicate with the LC
// instruments.


$force = 1
if ($# > 0) then
    if ($1 = 'update') then
        // Only do full parameter check if "lcpars" version has changed
        $force = 0
    else
        write('line3', 'Usage: lcpars[(\'update\')]')
    endif
endif

// Force full check if this "lcpars" version is not checked in
if ($version = '%\I%') then     // NB: Backslash keeps SCCS from changing this
    $force = 1
endif

exists('lcParamVersion','parameter','global'):$e
if (not $e) then
    create('lcParamVersion','string','global')
    lcParamVersion='n/a'
endif
setgroup('lcParamVersion','processing','global')
setprotect('lcParamVersion','set',32768+16+65536,'global')
if (lcParamVersion = $version and not $force) then
    return
endif
lcParamVersion=$version




// NB: lcMaxNumChans is used to initialize some arrayed parameters
exists('lcMaxNumChans','parameter','global'):$e
if (not $e) then
    create('lcMaxNumChans','real','global')
endif
setgroup('lcMaxNumChans','processing','global')
setprotect('lcMaxNumChans','set',32768+16+65536,'global')
lcMaxNumChans=5
setlimit('lcMaxNumChans',5,5,1,'global')         // This is a constant


exists('lcNumChans','parameter','global'):$e
if (not $e) then
    create('lcNumChans','real','global')
    setlimit('lcNumChans',5,0,1,'global')        // lcNumChans <= maxNumChans
    lcNumChans=5
endif
setgroup('lcNumChans','processing','global')
setprotect('lcNumChans','set',32768+16+65536,'global')


exists('vjDebugString','parameter','global'):$e
if (not $e) then
    create('vjDebugString','string','global')
    vjDebugString=''
endif
setgroup('vjDebugString','processing','global')
setprotect('vjDebugString','set',32768+16+65536,'global')


exists('lcValveToNmr','parameter','global'):$e
if ($e) then
    destroy('lcValveToNmr','global')
endif

exists('lcValveToColumn','parameter','global'):$e
if ($e) then
    destroy('lcValveToColumn','global')
endif

exists('lcPrinter','parameter','global'):$e
if (not $e) then
    create('lcPrinter','string','global')
    setgroup('lcPrinter','processing','global')
    lcPrinter='default','postscript'
endif
setgroup('lcPrinter','processing','global')
setprotect('lcPrinter','set',32768+16+65536,'global')

exists('lcPrintCommand','parameter','global'):$e
if (not $e) then
    create('lcPrintCommand','string','global')
    lcPrintCommand='lp'
endif
setgroup('lcPrintCommand','processing','global')
setprotect('lcPrintCommand','set',32768+16+65536,'global')

exists('lcAdcStartTime','parameter','global'):$e
if (not $e) then
    create('lcAdcStartTime','real','global')
    lcAdcStartTime=0
endif
setgroup('lcAdcStartTime','processing','global')
setprotect('lcAdcStartTime','set',32768+16+65536,'global')

exists('lcAdcStartDate','parameter','global'):$e
if (not $e) then
    create('lcAdcStartDate','string','global')
    lcAdcStartDate=''
endif
setgroup('lcAdcStartDate','processing','global')
setprotect('lcAdcStartDate','set',32768+16+65536,'global')


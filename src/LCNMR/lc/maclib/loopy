"macro loopy"

// Reconcile startloop and endloop parameters

// lcAnalyteCollector is the number of loops in the AC
if (startloop > lcAnalyteCollector) then startloop = 1 endif
if (endloop > lcAnalyteCollector) then endloop = 1 endif

if (endloop < startloop) then endloop = startloop endif

//lccmd('gotoLoop', startloop)

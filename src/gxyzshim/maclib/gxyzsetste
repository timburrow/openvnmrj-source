"macro gxyzsetste"

steflg='n'
$zgradtype=''
substr(gradtype,1,3):$zgradtype
if (tn='H1') then
   if ($zgradtype='h') then gxyzsetpars('H1HS') else gxyzsetpars('H1PFG') endif
elseif (tn='lk' or tn='H2') then
   if ($zgradtype='h') then gxyzsetpars('H2HS') else gxyzsetpars('H2PFG') endif
endif
dps



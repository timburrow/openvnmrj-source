#Main window
set helpTip(File) "Select printer or quit program"
set helpTip(Printer...) "Select printer and print options"
set helpTip(Quit) "Exit the acccounting program"

set helpTip(Edit) "Add or delete groups and users"
set helpTip(Add_Group) "Add accounting group"
set helpTip(Add_User) "Add users to a group"
set helpTip(Delete_Group) "Delete accounting group"
set helpTip(Delete_User) "Delete user from a group"

set helpTip(Holiday) "Assign holidays for billing purposes"
set helpTip(Add) "Display monthly calendar"

set helpTip(Recalc) "Recalculate accounts"
set helpTip(Update_Save) "Update and save accouting file, superuser only"
set helpTip(Print_all_groups) "Print charges for each group"
set helpTip(Print_main_window) "Print summary of charges"
set helpTip(Exit) "Exit the accounting program"
set helpTip(Console_Users) "Display list of all console users"
set helpTip(Console_Log) "Display log of console use"

#Printer Selection
set helpTip(Printer) "Type name of printer known to system"
set helpTip(Options) "Type print options, if any"
set helpTip(Print_string_will_be) "Default print command"
set helpTip(Apply) "Apply printer selection and options"
set helpTip(Cancel) "Quit window without saving selections"

#Console Users
set helpTip(Print_user) "Print log of selected user"
set helpTip(Cancel1) "Quit window without saving selections"
set helpTip(Print_user_windowx) ""

"macro Autoexmac"

     $maclib='' $macro=$1+'acq'
     if ($# > 1) then
	$maclib=$2
     else
        Automacrodir:$maclib
     endif
     $macfile=$maclib+'/'+$macro
     $parfile=$macfile+'pars'

     exists($parfile,'file'):$paramex
     if ($paramex) then
        fread($parfile)
     endif

     exists($macfile,'file'):$macroexist
     if ($macroexist) then
      	macrold($macfile):$dummy
      	exec($macro):$ret
      	purge($macfile)
     endif


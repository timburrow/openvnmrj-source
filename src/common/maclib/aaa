"macro aaa"
"aaa - abort acquisition with acqmode"

  exists('acqmode','parameter','global'):$e
  if $e<0.5 then
    killpshim:$dum
    aa
  elseif (acqmode='lock') then
    lock_scan('aa')
  elseif (acqmode='fidscan') then
    fid_scan('aa')
  else
    if (acqmode='prescan') then
      acqmode=''
    endif
    killpshim:$dum
    aa
  endif

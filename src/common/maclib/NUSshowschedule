"macro NUSshowschedule"
"NUSshowschedule"

$schedfile=curexp+'/sampling.sch'
$numincs='' $hts=''

on('nimax'):$e
if $e >0 then
  $numinc=nimax
else
  $numinc=trunc(CSdensity*ni)/100
endif
format($numinc,1,0):$numincs
$ht=1.05*$numinc
format($ht,1,0):$hts
shell('gnome-terminal --geometry 15x'+$hts+' -x tail -n '+$numincs+' -f '+$schedfile+' &'):$dum

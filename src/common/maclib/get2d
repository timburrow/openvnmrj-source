"macro get2d"
"  get2d  -   Returns 2D data to an experiment "
if ($# = 0) then
   $file = 'relayh'
else
   $file = $1
endif
$type = 'none'
exists(curexp+'/'+$file,'file'):$isthere
if ($isthere > 0.5) then
   if (seqfil = 's2pul') then
      tmpsave(tn)
   else
      tmpsave(seqfil)
   endif
   tmpreturn($file,'noflush'):$type
endif
return($type)

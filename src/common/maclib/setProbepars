"macro setProbepars"
if ($# < 1) then $1='' endif
$is=1
if ($1='force') then $is=0 endif
if ($is) then
   exists('probe_','parameter'):$is
endif
if ($is) then
   if (probe<>probe_) then $is=0 endif
endif

if ($is=0) then
   getparam('gradient','Probe'):PFGflg
   getparam('wtune','Probe'):wtune
   getparam('tunemethod','Probe'):tunemethod
   setGmap:Gmap
endif

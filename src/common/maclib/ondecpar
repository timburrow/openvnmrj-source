"macro ondecpar"
"ondecpar - on decoupler parameter state"
$ret=1
if $#>0.5 then
  $param=$1
  exists($param,'parameter'):$e
  if $e<0.5 then
    $ret=-1
  else
    $ret=0
    $value='' $value={$param}
    $ln=0 length($value):$ln
    if $ln<0.5 then
      $ret=1
    else
      $ss=''
      $i=0
      repeat
        $i=$i+1
        substr($value,$i,1):$ss
        if $ss<>'n' then
          $ret=1
        endif
      until ($i>=$ln)
    endif
  endif
endif
return($ret)

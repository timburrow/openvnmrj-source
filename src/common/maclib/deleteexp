"macro deleteexp"

$found=0
$expn=''

if ($# <> 2) then
  write('error','Usage deleteexp(start#,end#). macro aborted')
  abort
endif

$expi=$1
jexp:$curexp

        repeat
         format($expi,0,0):$expn
         $newexp='exp'+$expn
         exists(userdir+'/'+$newexp,'file'):$exist
         if ($expi=$curexp) then $exist=0 endif
         if ($exist) then
           delexp($expi)
         endif
         $expi=$expi+1
         until ($expi > $2)
         


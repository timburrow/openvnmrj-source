"macro _ipa"
" _ipa macro is called when ipa acquisition parameters are set "
if (acqmode = 'fidscan') then
  isspincad(seqfil,'quiet'):$e
  if ($e) then
    updtparam($1)
    setvalue($1,{$1},'processed')
  else
    aa('Updating display')
    $bs=bs on('bs'):$onbs
    bs=1
    wbs='fid_display'
    au('silent','bsclear','fidscan')
    acqmode='fidscan'
    bs=$bs if not $onbs then off('bs') endif
  endif
endif

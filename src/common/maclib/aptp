"macro aptp"
"*************************************"
"aptp - processing for apt"
"*************************************"
aptaph
if (pltmod = 'user') then         "plot with a user supplied macro"
   exists('userplapt','maclib'):$e
   if ($e > 0.5) then
      userplapt
   else
      write('error','macro userplapt must exist if pltmod = user')
   endif
else
   vsadjc(wc2max / 3.5)
   vp = wc2max / 2
   intmod='off'
   if (pltmod <> 'off') then
      pl
      apa
      page
   endif
endif

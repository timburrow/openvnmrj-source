"macro t1"
" ******************************************* 	"
" **** t1	-	t1 analysis	 **** 	"
" ******************************************* 	"

$x='  '
if $#>=1 then
   if (typeof('$1')) then
      substr($1,1,1):$ch
      if ($1='corr') or ($ch='c') then 
         $x='corr'
      endif
   endif
endif
"if $x='corr' t1 values are corrected by .03 sec for housekeeping errors"
analyze('expfit','d2','T1','list',$x)
cat(curexp+'/analyze.list')

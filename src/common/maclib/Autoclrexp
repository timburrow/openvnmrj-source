"macro Autoclrexp"
"***************************************************************"
"Autoclrexp   -   Cleans up the curexp with .def files, 	"
"			macdir files, eou* files and subexp	"
"***************************************************************"
cleanexp
if (auto <> 'y') then
 rm('-f',curexp+'/*.def'):$x
 Automacrodir('remove')
 rm('-rf',curexp+'/eou_saveas'):$x
 rm('-rf',curexp+'/eou_P*'):$x
 rm('-rf',curexp+'/eou_A*'):$x
endif

"macro aipShow"

"$file is fullpath "
"$frames = 'all' display all, otherwise use current layout."   
"$action = 'dnd' do not delete current data, otherwise deleteData." 



if ($# < 0.5) then
  $file = curexp+'/recon'
else
  $file = $1
endif


$frames = ''
if ($# > 1.5) then
   $frames = $2
endif

$action = ''
if ($# > 2.5) then
   $action = $3
endif

$e3D = 0
if ($# > 3.5) then
   $e3D = $4
endif

RQaction('loadData', $file, $action)

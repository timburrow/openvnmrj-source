"macro is_cpfid"
$arg2=''
if ($# < 1) then
    isnewexp:$new
    if ($new=1) then return(0) endif
    $1=file
endif
if ($1='exp') or ($1='') then return(0) endif
if ($# > 0.5) then
    substr($1,'dirname'):$arg2
endif
$filename='' $s1=''
substr($1,'basename'):$filename,$s1
$iscpfid=0
cplog($filename,'iscpfid',$arg2):$iscpfid
return($iscpfid)

"macro make_array"
 
  exists('a11','parameter'):$a11
   if ($a11 = 0) then 
      create('a11')
   endif
   exists('a12','parameter'):$a12
   if ($a11 = 0) then 
      create('a12')
   endif
   exists('a12','parameter'):$a13
   if ($a11 = 0) then 
      create('a13')
   endif
   exists('str1','parameter'):$str1
   if ($str1 = 0) then 
      create('str1','string')
   endif

   if str1<>'' then 
      array(str1,a11,a12,a13)
   endif

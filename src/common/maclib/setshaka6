"macro setshaka6"
if (dn <> 'H1') and (dn <> 'F19') then
  exists('pwxlvlS6','parameter'):$ex
  if ($ex) then
                "S6 pulse inverts approx. 3.3*gammab2"
        $gb2=260*dfrq*0.3
        getpower(pwxlvl-6,dn):$maxb2
        if ($maxb2 < $gb2) then
          $gb2=$maxb2
        endif
        setpower($gb2,dn):pwxlvlS6,pwxS6
  endif
endif


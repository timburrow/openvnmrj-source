"macro dps"

if ( $# = 0 ) then
  DPS
elseif ( $# = 1 ) then
  DPS($1)
elseif ( $# = 2 ) then
  DPS($1,$2)
elseif ( $# = 3 ) then
  DPS($1,$2,$3)
elseif ( $# = 4 ) then
  DPS($1,$2,$3,$4)
elseif ( $# = 5 ) then
  DPS($1,$2,$3,$4,$5)
elseif ( $# = 6 ) then
  DPS($1,$2,$3,$4,$5,$6)
elseif ( $# = 7 ) then
  DPS($1,$2,$3,$4,$5,$6,$7)
else
  DPS
endif

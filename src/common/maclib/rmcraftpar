"macro rmcraftpar"

        // Remove any potential craftpar/F1fidpars/BayesPars
$clist='' 
module('list','craftpar'):$clist
if $clist<>'' then 
    destroy($clist):$dum 
    destroy($clist,'processed'):$dum 
endif

$clist='' 
module('list','F1fidpars'):$clist
if $clist<>'' then 
    destroy($clist):$dum 
    destroy($clist,'processed'):$dum 
endif

$clist='' 
module('list','BayesPars'):$clist
if $clist<>'' then 
    destroy($clist):$dum 
    destroy($clist,'processed'):$dum 
endif


exists('aipDisplay','parameter','global'):$e
if (not $e) then
    create('aipDisplay','real','global')
    setgroup('aipDisplay','processing','global')
endif
if (size('aipDisplay') < 3) then 
    $i=0 $s=''
    repeat
      $i=$i+1
      format($i,1,0):$s
      $cmd='VP '+$s+' aipDisplay[1]=1 aipDisplay[2]=0 aipDisplay[3]=1'
      vnmrjcmd($cmd)
    until ($i >= jviewports[1])
endif
setgroup('aipDisplay','processing','global')
setprotect('aipDisplay','set',32768,'global')

exists('aipWindowSplit','parameter','global'):$e
if (not $e) then
    create('aipWindowSplit','real','global')
    setgroup('aipWindowSplit','processing','global')
endif
if (size('aipWindowSplit') < 2) then 
    $i=0 $s=''
    repeat
      $i=$i+1
      format($i,1,0):$s
      $cmd='VP '+$s+' aipWindowSplit[1]=0 aipWindowSplit[2]=0'
      vnmrjcmd($cmd)
    until ($i >= jviewports[1])
endif
setgroup('aipWindowSplit','processing','global')
setprotect('aipWindowSplit','set',32768,'global')


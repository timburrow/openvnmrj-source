endif
if (size('aipMovieSettings') < 5) then 
    $i=0 $s=''
    repeat
      $i=$i+1
      format($i,1,0):$s
      $cmd='VP '+$s+' aipMovieSettings[1]=1 aipMovieSettings[2]=1 aipMovieSettings[3]=1 aipMovieSettings[4]=100 aipMovieSettings[5]=100'
      vnmrjcmd($cmd)
    until ($i >= jviewports[1])
endif
setgroup('aipMovieSettings','processing','global')
setprotect('aipMovieSettings','set',32768,'global')


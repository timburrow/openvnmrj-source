"macro settune"
// set tune parameters

if (atune <> 'y') then
  write('error', 'Cannot run %s, tune hardware not available.', $0)
else
  rights('enableautosetup',0):$rt
  if (not $rt) then
    write('error', 'This operator is not allowed to run %s.', $0)
  else
    xmtune('addpar')
    vnmrjcmd('popup',
      'mode:modeless',
      'rebuild:yes',
      'file:Tune.xml',
      'location:topMenuBar',
      'title:Auto Tune Setup')
  endif
endif

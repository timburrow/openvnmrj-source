"macro T1process"
    wft select(arraydim)
    if (console='vnmrs') then
	aph0
    else
    	aph:$x           " phasing "
    	if $x<0.5 then
    	  aph0:$x
    	  if $x<0.5 then
    	    write('line3','Cannot phase spectrum')
    	  endif
    	endif
    endif
    ds(arraydim)
    vsadj
    thadj(wcmax/4,4)
    dpf dll fp t1s


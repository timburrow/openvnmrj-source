"macro cpmgt2"
"***********************************************************"
" cpmgt2 - converts s2pul into Carr-Purcell Meiboom-Gill T2 "
"***********************************************************"
psgset('cpmgt2','bt','dg','ap','ss','d2','spin')
pw=pw90
p1=2*pw90
d1=5*at 
gain='y'
if dm='y' then              "decoupler was on, switch mode"
   if waltz='y' then
      dmm='wcw'
   else
      dmm='fcf'
   endif
endif

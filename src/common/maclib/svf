"macro svf"
" svf - save data set"

$filename = ''
if ($# > 0.5) then
  $args = 'SVF(\''+$1
  $i = 1
  while ($i<$#) do
    $i = $i + 1
    $args = $args+'\',\''+${$i}
  endwhile
  $args = $args+'\')'
  exec($args)
  $filename = $1
else
  SVF:$filename
endif
cp(curexp+'/lcdata',$filename):$dum
cp(curexp+'/lcrunlog',$filename):$dum
dousermacro($0,$filename)

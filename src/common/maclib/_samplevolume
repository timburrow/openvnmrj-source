"macro _samplevolume"
"John Ryan"
"27 Feb 2014"

exists('molecularmass','parameter'):$pex
if $pex then
    exists('mass','parameter'):$pex
endif
if not $pex then return endif

if (molecularmass = 0) or (samplevolume = 0) then return endif

concentration = (mass * 1e6) / (molecularmass * samplevolume)


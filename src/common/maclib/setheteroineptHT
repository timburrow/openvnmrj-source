"macro setheteroineptHT"
exists('is_cpsample','maclib'):$cp
if (not $cp) then return endif

$pex=0 $ppath=''
AutoCheck($1):$pex,$ppath
getext($ppath):$ppath
if ($pex) then
   readparam($ppath+'.fid/procpar','htfrq1 htbw1')
   //setHTni
   rtv($ppath,'noabort','httof'):dof
   rtv($ppath,'noabort','htsw'):sw1
   dof=dof sw1=sw1
endif


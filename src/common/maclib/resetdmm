"macro resetdmm"
$dmm=''
getparam('dmm',dn):$dmm
if ($dmm <> '') then
  $subdmm=''
  $newdmm=''
  length(dmm):$dmmlength
  $i2 = 1
  repeat
   substr(dmm,$i2,1):$subdmm
   if ($subdmm = 'c') then
    $newdmm=$newdmm+'c'
   else
    $newdmm=$newdmm+$dmm
   endif
   $i2=$i2+1
  until $i2 > $dmmlength
  dmm=$newdmm
endif


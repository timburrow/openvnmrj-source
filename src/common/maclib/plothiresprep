"macro plothiresprep"
exists('topexp','parameter'):$ext
 if not $ext then create('topexp','string') endif
exists('sideexp','parameter'):$exs
 if not $exs then create('sideexp','string') endif
exists('plotcontours','parameter'):$exc
 if not $exc then create('plotcontours','string') endif
exists('plotcontourdiff','parameter'):$exd
 if not $exd then create('plotcontourdiff','string') endif

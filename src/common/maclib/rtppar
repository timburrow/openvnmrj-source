"macro rtppar"
if $#>0.5 then

exists($1,'parlib'):$e,$fpath
if ($e<0.5) then
  if ($## = 0) then
     write('error','%s: could not find \'%s\'',$0,$1)
     return
   else
     return(0)
   endif
endif

if ($# > 1.5) then
  rtp($fpath,$2)
else
  rtp($fpath)
endif
if ($## > 0) then
  return(1)
endif

endif

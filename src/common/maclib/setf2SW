"macro setf2SW"
exists('f2SW','parameter'):$ex
if ($ex=0) then return endif

if ($# < 2) then
  invsetsw2:$1,$2
endif
$s1='' $s2=''
format($1,'lower'):$s1
format($2,'lower'):$s2
if (refsource2 = 'dfrq') then
  f2SW='setsw2(dn,'+$s1+','+$s2+')'
elseif (refsource2 = 'dfrq2') then
  f2SW='setsw2(dn2,'+$s1+','+$s2+')'
elseif (refsource2 = 'sfrq') then
  f2SW='setsw2(tn,'+$s1+','+$s2+')'
else
  f2SW=''
endif

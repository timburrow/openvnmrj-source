"macro array2array"
"array to array separated variable"
"  Any element with nullstring will be dropped"

// If the arg1 is real, don't bother
if not typeof('$1') then return($1) endif

$total=size('$1')
$i=1 $j=1 $ret=''
repeat
    $s='' 
    string2array($1[$i]):$s
    if ($s[1]<>'') then
	$ret[$j]=$s
	$j=size('$ret')
	$j=$j+1
    endif
    $i=$i+1
until $i > $total
return($ret)

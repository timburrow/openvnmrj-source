"macro removeARRAYEDparam"
"									  "
" usage:  removeARRAYEDparam(paramName)                                   "
"									  "
if ($# < 1) then
   write('error','usage:  removeARRAYEDparam(paramName)')
   return(1)
endif

$paramName = $1

exists($paramName, 'parameter'):$b
if($b <> 0) then
   destroy($1)
endif

"macro movedssw"

$cr=cr
$delta=delta
if($#>1) then
  if($1>$2) then
    $cr=$1
    $delta=$1-$2
  else
    $cr=$2
    $delta=$2-$1
  endif
endif

addpar('downsamp')
dslsfrq = - ($cr - (sw/2 - rfl + rfp) - $delta/2.0)
downsamp = trunc(sw/$delta)
//if (downsamp < 2) then
//  downsamp=2
//endif

//cannot use downsamp and ssfilter together
exists('ssfilter','parameter'):$e
if($e) then
  off('ssfilter')
endif

"macro wetfrq2slp"
setwetpar
$wetfrqsize=size('wetfrq')

on('wetfrq'):$wfon
if ($wfon) then

  $i=1 $si=''
  repeat
    format($i-1,0,0):$si
    $par='slp'+$si
    {$par}=wetfrq[$i]-tof
    $par='slp'+$si+'bw'
    {$par}=wetfrqw[$i]
    $i=$i+1
  until $i > $wetfrqsize

  on('slp1'):$ison
  if ($ison) then
    slp=slp1 slpbw=slp1bw
    off('slp1')
  endif

endif

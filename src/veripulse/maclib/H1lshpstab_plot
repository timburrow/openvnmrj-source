"macro H1lshpstab_plot"
" Agilent H1 lshp stablibily plot "

select(1)
f full
res
rl(0)
axis=`h`
sp=-130
wp=260
ai
vsadj
vs=vs/5
vo=0.8
ho=0
dsww

$plotoptions='logo','header','plarray','comments_page1','miniplot','params'
doplotoption($plotoptions)

wexp:$e
if ($e) then
  page
  pltext
  page
endif

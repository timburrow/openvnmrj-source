"macro plottest"
setpage('newpage')

//Plot miniplot
setpage('miniplot','pl pscale')

//Plot parameters
//need to map template styles to plot panel
ap('ap',userdir+'/persistence/ap.dat',0)
setpage('parameter',userdir+'/persistence/ap.dat')

setpage('parameter',curexp+'/text')

pl
pscale
plottext

setpage('newpage')

//Plot miniplot
setpage('miniplot','pl pscale')
dli
setpage('integral',curexp+'/dli.out')

dll
setpage('peak',curexp+'/dll.out')

pageview



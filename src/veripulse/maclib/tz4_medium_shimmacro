"macro tz4_medium_shimmacro"
/* shimdss_shimmacro - maccro to return height of dss to spFidValue */

      fn=130000 
      lb=.6
      wft
      setref
      aph('select',-0.5p,0.5p)
      ai vs=10000 
      lb='n' wft tmsref
      sp=-0.5p wp=1.0p
      ds dc
      peak:$hgt,$frq
      write('line3','hgt = %3.1f',$hgt)
      return($hgt)



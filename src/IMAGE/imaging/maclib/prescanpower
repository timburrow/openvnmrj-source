"macro prescanpower"
"********************************************************************
"
"
" NAME:
"    prescanpower
"
" DESCRIPTION:
"    Set up parameters for:
"       PRESCAN - Transmitter Power Adjustment
"
"********************************************************************
rtppar($0,'nomenu'):$e
if ($e=0) then
  write('error','No parameters for pulse sequence %s',$0)
endif
setgss('gss','thk')

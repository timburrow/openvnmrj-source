"macro epimss_prep"
" ******************************* "
" *** epimss_prep - prep macro*** "
" ******************************* "

setloop

if(nseg = 1) then
  if (petable = '') or (petable = 'n') then
    return
  else
    petable = ''
  endif
else
  if (petable = '') or (petable = 'n') then
    write('error','ERROR: phase table (petable) not set')
    abort
  endif 
endif
    

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="AutoHT" element="pages" type="acquisition" >
  <group size="710 280"
    style="PlainText"
    label="AutoHT"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="AutoHT"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 0" size="210 20"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <textmessage loc="5 0" size="135 20"
        style="Heading5"
        label="autoHT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <button loc="405 135" size="80 20"
      style="Heading1"
      label="Begin"
      vc="autoHT('start')"
      bg="transparent"
      decor1="no"
      />
    <entry loc="370 185" size="295 20"
      style="PlainText"
      vq="svfdir"
      vc="svfdir='$VALUE' $s='' $d='' strstr(svfdir,'/','last'):$r,$s,$d if($d='') then svfdir=$s endif"
      set="$VALUE=svfdir"
      disable="Grayed out"
      />
    <label loc="240 185" size="130 20"
      style="Heading2"
      label="Storage Directory"
      vq="svfdir"
      show="exists(svfdir,'directory'):$ENABLE"
      justify="Left"
      />
    <group loc="240 55" size="430 55"
      style="Heading2"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ObserveChannel"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="195 10" size="80 20"
        style="PlainText"
        label="HT linewidth"
        justify="Left"
        />
      <label loc="45 30" size="60 20"
        style="PlainText"
        label="at power"
        justify="Left"
        />
      <check loc="200 30" size="105 20"
        style="Menu4"
        label="1D INEPT"
        vq="HTexp HTch"
        vc="HTch='y'"
        vc2="HTch='n'"
        set="if (HTch='y') then $VALUE=1 else $VALUE=0 endif"
        show="if ( HTexp[6]='hsqcHT') or (HTexp[7]='hsqctocsyHT') then $ENABLE=1 else $ENABLE=0 endif"
        />
      <check loc="345 30" size="60 20"
        style="Menu4"
        label="plot"
        vq="HTplot"
        vc="HTplot='y'"
        vc2="HTplot='n'"
        set="if (HTplot='y') then $VALUE=1 else $VALUE=0 endif"
        toolTip="Automatically plot Hadamard spectra"
        />
      <label loc="310 10" size="20 20"
        style="PlainText"
        label="Hz"
        justify="Left"
        />
      <entry loc="275 10" size="35 20"
        style="PlainText"
        vq="lb1"
        vc="lb1=$VALUE"
        set="$VALUE=lb1"
        show="if (lb1=0) and (ct &gt;0) then $VALUE=0 else $VALUE=1 endif"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 10" size="100 20"
        style="PlainText"
        label="H-1 Pulse Width"
        justify="Left"
        />
      <label loc="140 30" size="20 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <label loc="140 10" size="20 20"
        style="PlainText"
        label="us"
        justify="Left"
        />
      <entry loc="105 30" size="35 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        disable="Grayed out"
        />
      <entry loc="105 10" size="35 20"
        style="PlainText"
        vq="HTpw"
        vc="HTpw=$VALUE"
        set="$VALUE=HTpw"
        show="if (HTpw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
    </group>
    <group loc="240 20" size="430 30"
      style="Heading2"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ObserveChannel"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="355 5" size="55 20"
        style="Label1"
        label="H1noe.ll"
        vq="svfdir "
        show="exists(svfdir+'/H1noe.ll', 'file'):$VALUE $ENABLE=$VALUE"
        justify="Center"
        />
      <label loc="320 5" size="25 20"
        style="PlainText"
        label="and"
        justify="Left"
        />
      <label loc="275 5" size="35 20"
        style="Label1"
        label="CH.ll"
        vq="svfdir "
        show="exists(svfdir+'/CH.ll','file'):$VALUE $ENABLE=$VALUE"
        justify="Center"
        />
      <label loc="225 5" size="40 20"
        style="Label1"
        label="C13.ll"
        vq="svfdir "
        show="exists(svfdir+'/C13.ll','file'):$VALUE $ENABLE=$VALUE"
        justify="Center"
        />
      <label loc="180 5" size="35 20"
        style="Label1"
        label="H1.ll"
        vq="svfdir "
        show="exists(svfdir+'/H1.ll','file'):$VALUE $ENABLE=$VALUE"
        justify="Center"
        />
      <label loc="10 0" size="160 30"
        style="Heading2"
        label="Optional HT Lists:"
        justify="Left"
        />
    </group>
    <group loc="5 20" size="210 255"
      style="Heading2"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ObserveChannel"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="45 165" size="90 20"
        style="Label3"
        label="CH2 negative"
        justify="Left"
        />
      <label loc="70 125" size="25 20"
        style="Label3"
        label="all"
        justify="Left"
        />
      <label loc="45 145" size="55 20"
        style="Label3"
        label="CH only"
        justify="Left"
        />
      <check loc="25 165" size="20 20"
        style="Heading4"
        vq="HTexp"
        vc="HTexp[8]='hsqcHT'"
        vc2="HTexp[8]='n'"
        set="if (HTexp[8]='hsqcHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="25 145" size="20 20"
        style="Heading4"
        vq="HTexp"
        vc="HTexp[7]='hsqcHT'"
        vc2="HTexp[7]='n'"
        set="if (HTexp[7]='hsqcHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <textmessage loc="140 165" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=8) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=8) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 145" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=7) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=7) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 125" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=6) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=6) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <check loc="5 125" size="65 20"
        style="Heading4"
        label="HSQC"
        vq="HTexp[6] HTexp[7] HTexp[8]"
        vc="HTexp[6]='hsqcHT'"
        vc2="HTexp[6]='n' HTexp[7]='n' HTexp[8]='n'"
        set="if (HTexp[6]='hsqcHT') or (HTexp[7]='hsqcHT') or (HTexp[8]='hsqcHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="160 0" size="50 250"
        style="Heading2"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ObserveChannel"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <entry loc="10 165" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[8]=$VALUE"
          set="$VALUE=HTnt[8]"
          disable="Grayed out"
          />
        <entry loc="10 145" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[7]=$VALUE"
          set="$VALUE=HTnt[7]"
          disable="Grayed out"
          />
        <entry loc="10 225" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[11]=$VALUE"
          set="$VALUE=HTnt[11]"
          disable="Grayed out"
          />
        <entry loc="10 205" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[10]=$VALUE"
          set="$VALUE=HTnt[10]"
          disable="Grayed out"
          />
        <entry loc="10 185" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[9]=$VALUE"
          set="$VALUE=HTnt[9]"
          disable="Grayed out"
          />
        <entry loc="10 125" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[6]=$VALUE"
          set="$VALUE=HTnt[6]"
          disable="Grayed out"
          />
        <entry loc="10 105" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[5]=$VALUE"
          set="$VALUE=HTnt[5]"
          disable="Grayed out"
          />
        <entry loc="10 85" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[4]=$VALUE"
          set="$VALUE=HTnt[4]"
          disable="Grayed out"
          />
        <entry loc="10 65" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[3]=$VALUE"
          set="$VALUE=HTnt[3]"
          disable="Grayed out"
          />
        <entry loc="10 45" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[2]=$VALUE"
          set="$VALUE=HTnt[2]"
          disable="Grayed out"
          />
        <entry loc="10 25" size="30 20"
          style="PlainText"
          vq="HTnt"
          vc="HTnt[1]=$VALUE"
          set="$VALUE=HTnt[1]"
          disable="Grayed out"
          />
        <label loc="5 5" size="40 20"
          style="Label1"
          label="scans"
          justify="Left"
          />
      </group>
      <check loc="5 225" size="120 20"
        style="Heading4"
        label="HETCOR"
        vq="HTexp"
        vc="HTexp[11]='hetcorHT'"
        vc2="HTexp[11]='n'"
        set="if (HTexp[11]='hetcorHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 205" size="105 20"
        style="Heading4"
        label="HMBC"
        vq="HTexp"
        vc="HTexp[10]='hmbcHT'"
        vc2="HTexp[10]='n'"
        set="if (HTexp[10]='hmbcHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 185" size="115 20"
        style="Heading4"
        label="HSQC TOCSY"
        vq="HTexp"
        vc="HTexp[9]='hsqctocsyHT'"
        vc2="HTexp[9]='n'"
        set="if (HTexp[9]='hsqctocsyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <textmessage loc="140 225" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=11) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=11) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 205" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=10) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=10) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 185" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=9) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=9) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 25" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=1) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=1) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 45" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=2) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=2) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 65" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=3) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=3) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 85" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=4) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=4) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="140 105" size="20 20"
        style="Heading5"
        label=" "
        vq="HTn"
        set="if(HTn=5) then $VALUE='&lt;' else $VALUE=' ' endif"
        show="if (HTn=5) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <radio loc="100 125" size="35 20"
        style="PlainText"
        label="ZZ"
        vq="HTzz"
        vc="HTzz='y'"
        vc2="HTzz='n'"
        set="$VALUE=(HTzz='y')"
        toolTip="ZZ-filter"
        />
      <check loc="5 105" size="105 20"
        style="Heading4"
        label="ROESY"
        vq="HTexp"
        vc="HTexp[5]='roesyHT'"
        vc2="HTexp[5]='n'"
        set="if (HTexp[5]='roesyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 45" size="85 20"
        style="Heading4"
        label="COSY"
        vq="HTexp"
        vc="HTexp[2]='gcosyHT'"
        vc2="HTexp[2]='n'"
        set="if (HTexp[2]='gcosyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 25" size="85 20"
        style="Heading4"
        label="TOCSY"
        vq="HTexp"
        vc="HTexp[1]='tocsyHT'"
        vc2="HTexp[1]='n'"
        set="if (HTexp[1]='tocsyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 65" size="105 20"
        style="Heading4"
        label="DQ COSY"
        vq="HTexp"
        vc="HTexp[3]='dqcosyHT'"
        vc2="HTexp[3]='n'"
        set="if (HTexp[3]='dqcosyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 85" size="105 20"
        style="Heading4"
        label="NOESY"
        vq="HTexp"
        vc="HTexp[4]='noesyHT'"
        vc2="HTexp[4]='n'"
        set="if (HTexp[4]='noesyHT') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="5 5" size="120 20"
        style="Heading2"
        label="HT Experiments"
        justify="Left"
        />
    </group>
  </group>
</template>

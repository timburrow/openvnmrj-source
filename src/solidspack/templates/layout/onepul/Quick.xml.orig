<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading2"
    label="Defaults"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <button loc="140 180" size="130 20"
      style="Label3"
      label="Delete Module"
      vc="exists('SPregionfilename3','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename3&lt;&gt;'') then rm(userdir+'/modules/'+SPregionfilename1):$dum  write('line3','%s Deleted',SPregionfilename3) SPregionfilename3='' else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename3','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <button loc="140 100" size="130 20"
      style="Label3"
      label="Edit Module"
      vc="exists('SPregionfilename2','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename2&lt;&gt;'') then regionmodule('makemod',2) else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename2','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <button loc="140 160" size="130 20"
      style="Label3"
      label="Edit Module"
      vc="exists('SPregionfilename3','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename3&lt;&gt;'') then regionmodule('makemod',3) else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename3','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <entry loc="140 140" size="130 20"
      style="Label3"
      vq="SPregionfilename3"
      vc="SPregionfilename3='$VALUE' "
      set="$VALUE=SPregionfilename3"
      show="exists('SPregionfilename3','parameter','global'):$VALUE"
      disable="Grayed out"
      />
    <entry loc="140 80" size="130 20"
      style="Label3"
      vq="SPregionfilename2"
      vc="SPregionfilename2='$VALUE' "
      set="$VALUE=SPregionfilename2"
      show="exists('SPregionfilename2','parameter','global'):$VALUE"
      disable="Grayed out"
      />
    <label loc="10 140" size="130 20"
      style="Heading4"
      label="Region Module 3:"
      />
    <button loc="140 120" size="130 20"
      style="Label3"
      label="Delete Module"
      vc="exists('SPregionfilename2','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename2&lt;&gt;'') then rm(userdir+'/modules/'+SPregionfilename2):$dum  write('line3','%s Deleted',SPregionfilename2) SPregionfilename2='' else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename2','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <label loc="10 80" size="130 20"
      style="Heading4"
      label="Region Module 2:"
      />
    <button loc="140 60" size="130 20"
      style="Label3"
      label="Delete Module"
      vc="exists('SPregionfilename1','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename1&lt;&gt;'') then rm(userdir+'/modules/'+SPregionfilename1):$dum  write('line3','%s Deleted',SPregionfilename) SPregionfilename1='' else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename1','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <button loc="140 40" size="130 20"
      style="Label3"
      label="Edit Module"
      vc="exists('SPregionfilename1','parameter','global'):$e if ($e&gt;0.5) then if (SPregionfilename1&lt;&gt;'') then regionmodule('makemod',1) else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPregionfilename1','global'):$VALUE"
      bg="transparent"
      decor1="no"
      />
    <label loc="10 20" size="130 20"
      style="Heading4"
      label="Region Module 1:"
      />
    <entry loc="140 20" size="130 20"
      style="Label3"
      vq="SPregionfilename1"
      vc="SPregionfilename1='$VALUE' "
      set="$VALUE=SPregionfilename1"
      show="exists('SPregionfilename1','parameter','global'):$VALUE"
      disable="Grayed out"
      />
    <textmessage loc="90 0" size="170 20"
      style="Label3"
      label="onepul"
      vq="seqfil"
      set="$VALUE=seqfil"
      />
    <label loc="10 0" size="80 20"
      style="Heading4"
      label="Sequence:"
      />
  </group>
</template>

"macro mqf1shear"
exists('ival','parameter'):$ival
if ($ival=0) then 
   create('ival')
   ival=0.5
endif

if (ival=0.5) then 
   daslp=0.0
endif 

if (ival=1.5) then
   daslp=-(7/9)*360.0*(sw/sw1)
endif

if (ival=2.5) then
   daslp=(19/12)*360.0*(sw/sw1)
endif

if (ival=3.5) then
   daslp=(101/45)*360.0*(sw/sw1)
endif

if (ival=4.5) then
   daslp=(91/36)*360.0*(sw/sw1)
endif


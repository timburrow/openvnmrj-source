"macro mqf1shear5"
exists('ival','parameter'):$ival
if ($ival=0) then 
   create('ival')
   ival=0.5
endif

if ((ival=0.5) or (ival = 1.5)) then 
   daslp=0.0
endif 

if (ival=2.5) then
   daslp=-(25/12)*360.0*(sw/sw1)
endif

if (ival=3.5) then
   daslp=(11/9)*360.0*(sw/sw1)
endif

if (ival=4.5) then
   daslp=(95/36)*360.0*(sw/sw1)
endif



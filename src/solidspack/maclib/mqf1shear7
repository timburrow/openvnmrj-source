"macro mqf1shear7"

exists('ival','parameter'):$ival
if ($ival=0) then 
   create('ival')
   ival=0.5
endif

if ((ival=0.5) or (ival = 1.5)or (ival = 2.5)) then 
   daslp=0.0
endif 

if (ival=3.5) then
   daslp=-(161/45)*360.0*(sw/sw1)
endif

if (ival=4.5) then
   daslp=(7/18)*360.0*(sw/sw1)
endif



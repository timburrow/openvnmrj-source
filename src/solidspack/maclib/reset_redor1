"macro reset_redor1"
// Set Parameters 

aXxyxy8 = aX90
aYxyxy8 = aY90
pwXxyxy8 = 2.0*pwX90
pwYxyxy8 = 2.0*pwY90
fXYxy8 = 0.5
nXYxy8 = 32
onYxyxy8 = 1,2

// Set Decoupling For REDOR

exists('Hseq','parameter'):$Hseq
if ($Hseq>0.5) then 
   Hmixseq = Hseq
   aHmixspinal = aHspinal
   aHmixtppm = aHtppm 
   pwHmixspinal = pwHspinal
   pwHmixtppm = pwHtppm 
   phHmixspinal = phHspinal
   phHmixtppm = phHtppm 
endif   









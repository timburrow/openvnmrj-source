"macro reset_redor2"
// Set Parameters 

aYxy8 = aY90
pwYxy8 = 2.0*pwY90
nYxy8 = 32
onYxy8 = 1,2
aX180 = aX90
pwX180 = 2.0*pwX90

// Set Decoupling For REDOR

exists('Hseq','parameter'):$Hseq
if ($Hseq>0.5) then 
   Hmixseq = Hseq
   aHmixspinal = aHspinal
   aHmixtppm = aHtppm 
   pwHmixspinal = pwHspinal
   pwHmixtppm = pwHtppm 
   phHmixspinal = phHspinal
   phHmixtppm = phHtppm 
endif   








